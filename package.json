{
  "name": "dext-starter",
  "version": "1.0.0",
  "description": "My badass Nuxt.js project",
  "author": "BartKnucle",
  "bin": "server/index.js",
  "private": true,
  "config": {
    "nuxt": {
      "host": "0.0.0.0",
      "port": "3333"
    }
  },
  "scripts": {
    "dev-debug": "node --inspect node_modules/.bin/nuxt dev",
    "dev-debug-win": "node --inspect node_modules/nuxt/bin/nuxt dev",
    "_dev-debug-win": "set LOG=debug && node --inspect node_modules/nuxt/bin/nuxt dev --no-lock",
    "build": "nuxt build",
    "start": "nuxt start",
    "start1": "node server/index.js",
    "generate": "nuxt generate --analyse",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "@nuxtjs/pwa": "^3.0.0-beta.16",
    "browser-or-node": "^1.2.1",
    "cross-env": "^5.2.0",
    "ipfs": "^0.34.4",
    "json-stable-stringify": "^1.0.1",
    "libp2p-pnet": "^0.1.0",
    "libp2p-webrtc-star": "^0.16.1",
    "libp2p-websocket-star-rendezvous": "^0.3.0",
    "nuxt": "^3.4.3",
    "orbit-db-win": "^0.21.4",
    "platform": "^1.3.5",
    "rxjs": "^6.5.1",
    "vuetify": "^1.5.12",
    "winston": "^3.1.0",
    "wrtc": "^0.4.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "eslint": "^5.0.1",
    "eslint-config-prettier": "^3.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-vue": "^4.0.0",
    "prettier": "1.14.3",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2"
  }
}
