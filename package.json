{
  "name": "dext-starter",
  "version": "1.0.0",
  "description": "My badass Nuxt.js project",
  "author": "BartKnucle",
  "private": true,
  "config": {
    "nuxt": {
      "host": "0.0.0.0",
      "port": "3000"
    }
  },
  "scripts": {
    "dev-debug": "node --inspect node_modules/.bin/nuxt dev",
    "dev-debug-win": "node --inspect node_modules/nuxt/bin/nuxt dev",
    "_dev-debug-win": "set LOG=debug && node --inspect node_modules/nuxt/bin/nuxt dev --no-lock",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate --analyse",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "auto-bind": "^2.0.0",
    "browser-or-node": "^1.1.0",
    "cross-env": "^5.2.0",
    "ipfs": "^0.33.1",
    "libp2p-websocket-star-rendezvous": "^0.3.0",
    "nuxt": "^2.4.2",
    "orbit-db": "^0.19.9",
    "platform": "^1.3.5",
    "vuetify": "^1.2.4",
    "winston": "^3.1.0",
    "wrtc": "^0.3.5"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "eslint": "^5.0.1",
    "eslint-config-prettier": "^3.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-vue": "^4.0.0",
    "prettier": "1.14.3",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2"
  }
}
