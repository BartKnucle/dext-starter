{
  "name": "dext-starter",
  "version": "1.0.0",
  "description": "My badass Nuxt.js project",
  "author": "BartKnucle",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --inspect-brk server/index.js --watch server",
    "dev-debug": "node --inspect node_modules/.bin/nuxt dev",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.0.0",
    "cross-env": "^5.2.0",
    "dgram": "^1.0.1",
    "express": "^4.16.3",
    "go-ipfs-dep": "^0.4.18",
    "ipfs": "^0.33.1",
    "ipfs-api": "^26.1.2",
    "ipfsd-ctl": "^0.40.2",
    "libp2p-mdns": "^0.12.1",
    "net": "^1.0.2",
    "nuxt": "^2.0.0",
    "orbit-db": "^0.19.9",
    "vuetify": "^1.2.4"
  },
  "devDependencies": {
    "nodemon": "^1.11.0",
    "babel-eslint": "^8.2.1",
    "eslint": "^5.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-vue": "^4.0.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-prettier": "2.6.2",
    "prettier": "1.14.3",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2"
  }
}
